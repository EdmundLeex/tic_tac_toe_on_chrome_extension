{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'",
  "name": "Super Tic Tac Toe",
  "version": "0.0.1",
  "description": "Tic Tac Toe of Strategy",
  // "homepage_url": "#",
  "minimum_chrome_version": "46",
  // "icons": {
  //   "128": "assets/images/icon-lg.png"
  // },
  "permissions": [
    "notifications",
    "cookies",
    "alarms",
    "storage",
    "tabs",
    "http://localhost:3000/"

  ],
  "optional_permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    // "default_icon": {
    //   "38": "assets/images/icon.png"
    // },
    "default_title": "Super Tic Tac Toe",
    "default_popup": "popup.html"
  }
}
